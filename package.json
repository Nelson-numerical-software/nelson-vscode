{
    "name": "nelson",
    "displayName": "Nelson",
    "description": "Nelson support for Visual Studio Code",
    "version": "2.1.0",
    "publisher": "Nelson-numerical-software",
    "license": "GPL-2.0",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Nelson-numerical-software/nelson-vscode"
    },
    "icon": "images/nelson.png",
    "bugs": "https://github.com/Nelson-numerical-software/nelson-vscode/issues",
    "contributes": {
        "languages": [
            {
                "id": "nelson",
                "aliases": [
                    "Nelson",
                    "nelson"
                ],
                "extensions": [
                    ".m",
                    ".nlf",
                    ".nls"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "nelson",
                "scopeName": "source.nelson",
                "path": "./syntaxes/nelson.tmLanguage"
            }
        ],
        "snippets": [
            {
                "language": "nelson",
                "path": "./snippets/nelson.json"
            }
        ]
    }
}
